import java.util.Scanner;

public class Remarks {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.print("Enter the score: ");
        int score = scanner.nextInt();
        
        if (score < 0 || score > 100) {
            System.out.println("Remark: Invalid Score");
        } else if (score < 65) {
            System.out.println("Remark: Invalid Score");
        } else if (score <= 74) {
            System.out.println("Remark: Failed");
        } else if (score <= 80) {
            System.out.println("Remark: Not Bad");
        } else if (score <= 85) {
            System.out.println("Remark: Good");
        } else if (score <= 90) {
            System.out.println("Remark: Very Good");
        } else if (score <= 95) {
            System.out.println("Remark: Satisfactory");
        } else if (score <= 99) {
            System.out.println("Remark: Excellent");
        } else {
            System.out.println("Remark: Perfect");
        }
        scanner.close();
    }
}
