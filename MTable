import java.util.Scanner;

public class MTable {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.print("Enter Multiplication Table Size: ");
        int Laki = scanner.nextInt();

        int i = 1;
        while (i <= Laki) {
            int j = 1;
            while (j <= Laki) {
                System.out.print(i * j + "\t");
                j++;
            }
            System.out.println();
            i++;
        }

        scanner.close();
    }
}
